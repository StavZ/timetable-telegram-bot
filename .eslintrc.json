{
  "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "google"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
      "linebreak-style": "off",
      "comma-dangle": [
          "error",
          "never"
      ],
      "valid-jsdoc": "off",
      "max-len": "off",
      "indent": [
          "error",
          2,
          {
              "SwitchCase": 1
          }
      ],
      "object-curly-spacing": "off",
      "space-before-function-paren": "off",
      "semi": "error",
      "no-console": "error",
      "no-extra-semi": "error",
      "require-jsdoc": "off",
      "no-unused-vars": "warn"
  }
}